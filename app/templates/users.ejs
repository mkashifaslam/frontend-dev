<% include app/templates/nav %>

    <div class="container-fluid">
      <div class="content-container">
         <h2>Users</h2>
         <div class="table-responsive">
         <table class="table bg-white">
          <thead>
            <tr>
              <th>#</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Username</th>
              <th>Role</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < users.length; i++) { %>
            <% var user = users[i]; %>  
              <tr>
                <th scope="row"><%= i+1 %></th>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.email %></td>
                <td><%= user.role %></td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      </div>
  </div>